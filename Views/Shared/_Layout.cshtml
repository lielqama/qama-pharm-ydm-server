<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>סנכרון משלוחים - לוגיסטיק בריין</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body  dir="rtl">
    <div class="navbar navbar-inverse navbar-fixed-top ">
        <div class="container" >
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("סנכרון לחברות הפצה", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse navbar-left">
                <ul class="nav navbar-nav">
                    <li>@*@Html.ActionLink("שחזור מידע ממדבקות", "RecoverStikeData", "Home", new { area = "" }, null)*@</li>
                    <li>@Html.ActionLink("לוגים", "Index", "Home", new { area = "" }, null)</li>

                    <li>@Html.ActionLink("קודים - PharmYdm", "Index", "HFDQamaClients", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("קודים - בר", "Index", "BarQamaClients", new { area = "" }, null)</li>


                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Tzurtech</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
